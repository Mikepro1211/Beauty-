<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="sv.edu.udb.beautyapp">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher_beauty"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_beauty"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        tools:targetApi="31">
        <activity
            android:name=".Facturacion"
            android:exported="false"
            android:theme="@style/Theme.BeautyApp"/>
        <activity
            android:name=".Productos"
            android:exported="false"
            android:theme="@style/Theme.BeautyApp"/>
        <activity
            android:name=".Sucursale"
            android:exported="false"
            android:theme="@style/Theme.BeautyApp"/>
        <activity
            android:name=".RegistrarCita"
            android:exported="false"
            android:theme="@style/Theme.BeautyApp"/>
        <activity
            android:name=".Registro"
            android:exported="false"
            android:theme="@style/Theme.BeautyApp" />
        <activity
            android:name=".Login"
            android:exported="false"
            android:theme="@style/Theme.BeautyApp" />
        <activity
            android:name=".SplashActivity"
            android:exported="true"
            android:theme="@style/Theme.BeautyApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:theme="@style/Theme.BeautyApp" />
    </application>

</manifest>